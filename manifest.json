{
  "manifest_version": 2,
  "name": "YouTube/IMSLP",
  "version": "1.0",
  "description": "search for 'composer' + 'music title'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon":  "icon.png",
	"default_title": "YouTube/IMSLP"
  },
  "permissions": [
    "tabs",
	"http://imslp.org/wiki/*"
  ],
  "icons" : {
    "128": "icon.png"
  }
}