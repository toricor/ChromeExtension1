{
  "manifest_version": 2,
  "name": "search YouTube from IMSLP",
  "version": "1.0",
  "description": "search 'youtube' + 'composer' + 'music title'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon":  "icon.png",
	"default_title": "YouTube Search from IMSLP"
  },
  "permissions": [
    "tabs",
	"http://imslp.org/wiki/*"
  ],
  "icons" : {
    "128": "icon.png"
  }
}